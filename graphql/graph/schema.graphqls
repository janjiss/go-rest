# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  email: String!
  name: String!
}

type Query {
  users: [User!]!
}

input CreateUserInput {
  email: String!
  name: String!
}

type CreateUserPayload {
  user: User!
}

type Mutation {
  createUser(input: CreateUserInput!): CreateUserPayload!
}

